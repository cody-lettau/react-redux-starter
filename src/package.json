    "webpack-dev-server": "1.15.1",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-register": "6.9.0",
    "casual": "1.5.3",
    "chai": "3.5.0",
    "dirty-chai": "1.2.2",
    "enzyme": "2.3.0",
    "eslint": "3.4.0",
    "eslint-config-airbnb": "10.0.1",
    "eslint-html-reporter": "0.5.2",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.1",
    "eslint-plugin-react": "6.2.0",
    "file-loader": "0.9.0",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.5.0",
    "json-loader": "0.5.4",
    "mocha": "3.0.2",
    "mocha-teamcity-reporter": "1.1.1",
    "react-addons-test-utils": "15.3.1",
    "redux-mock-store": "1.1.4",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "sinon-stub-promise": "3.0.1",
    "supertest": "2.0.0",
    "timekeeper": "0.1.1"
  },
  "scripts": {
    "build": "npm run build-client && npm run build-server && npm run build-client-debug",
    "build-client": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --colors --hot --inline --display-error-details --config client/webpack.config.js",
    "build-client-debug": "NODE_ENV=dev node_modules/webpack/bin/webpack.js -p --progress --colors --hot --inline --display-error-details --config client/webpack.config.js",
    "build-server": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --colors --hot --inline --display-error-details --config server/webpack.config.js",
    "start": "PORT=7000 HTML_PATH=../build/public JS_PATH=../build/public NODE_ENV=production node ../build/server_bundle.js",
    "start-dev": "NODE_ENV=dev PORT=7000 HTML_PATH=../build/public JS_PATH=../build/public node ../build/server_bundle.js",
    "dev": "NODE_ENV=dev PORT=7000 HTML_PATH=client/ JS_PATH=../build/public nodemon server/index.js",
    "dev-server": "npm run build-client-devserver && npm run start-dev-server",
    "build-client-devserver": "NODE_ENV=devserver ENABLE_LINTER=true node_modules/webpack/bin/webpack.js -p --progress --colors --hot --inline --display-error-details --config client/webpack.config.js",
    "start-dev-server": "NODE_ENV=dev ENABLE_LINTER=true node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors --hot --inline --display-error-details --config client/webpack.config.js",
    "lint": "npm run lint-server && npm run lint-client",
    "lint-server": "eslint -c server/.eslintrc.js --ignore-path server/.eslintignore server",
    "lint-client": "eslint -c client/.eslintrc.js --ignore-path client/.eslintignore client --ext .js --ext .jsx",
    "lint-reports": "npm run lint-server-report && npm run lint-client-report",
    "lint-server-report": "eslint -c server/.eslintrc.js --ignore-path server/.eslintignore -f node_modules/eslint-html-reporter/reporter-team-city.js -o linting/reports/server.html server",
    "lint-client-report": "eslint -c client/.eslintrc.js --ignore-path client/.eslintignore -f node_modules/eslint-html-reporter/reporter-team-city.js -o linting/reports/client.html client --ext .js --ext .jsx",
    "test": "mocha --timeout 5000 --compilers js:babel-core/register --require ./test/helpers/setup-helpers.js --recursive test",
    "test:watch": "npm run test -- --watch",
    "cover": "istanbul cover _mocha -- --timeout 5000 --compilers js:babel-core/register --require ./test/helpers/setup-helpers.js --recursive test",
    "cover:teamcity": "istanbul cover _mocha --report teamcity -- --reporter node_modules/mocha-teamcity-reporter/lib/teamcity.js --compilers js:babel-core/register --require ./test/helpers/setup-helpers.js --recursive test"
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ],
    "env": [
      "mocha"
    ]
  },
  "author": "",
  "license": "ISC"
}
